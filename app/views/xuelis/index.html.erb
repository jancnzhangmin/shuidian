<%= render 'left' %>
<%= render 'top' %>








<p></p>





<div class="col-lg-12">
  <div class="ibox float-e-margins">
    <div class="ibox-title">
      <h5>学历管理</h5>

    </div>
    <div class="ibox-content">

      <table class="table table-hover table-bordered">
        <thead>
        <tr>
          <th>学历</th>
          <th>操作</th>
        </tr>
        </thead>
        <tbody>
        <% @xuelis.each do |xueli| %>
            <tr>
              <td>
                <%= xueli.xueli %>
              </td>
              <td>
                <%= link_to "", edit_xueli_path(xueli),class: "fa fa-pencil" %>&nbsp;
                <%= link_to '', xueli, class: "fa fa-trash", method: :delete, data: { confirm: 'Are you sure?' } %>
              </td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <%= link_to "新增", new_xueli_path,class: "btn btn-w-m btn-success mylink pull-right" %>
      <br>
    </div>

  </div>
</div>